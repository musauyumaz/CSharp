@using TravelTripProje.Models.Sınıflar
@model List<Blog>

<div class="box1-left">
    @foreach (var blog in Model)
    {

   
    <div class="bot-img">
        <img src=@blog.BlogImage alt="" />
    </div>
    <div class="bot-desc1">
        <p>@blog.Aciklama.Substring(0,80)</p>
    </div>
    <div class="clear"></div>
    }
</div>

